# NocoDB API Configuration
NOCODB_BASE_URL=https://your-nocodb-instance.com
NOCODB_API_TOKEN=your-api-token
NOCODB_PROJECT_ID=your-project-id

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017
MONGODB_DATABASE=conversation_analytics

# Table/Collection Names
NOCODB_CONVERSATION_TABLE=Conversation
NOCODB_MESSAGES_TABLE=Messages

# MongoDB to NocoDB Sync Configuration
# Uncomment and set these values to customize the sync process
# SYNC_BATCH_SIZE=100
# SYNC_STATE_FILE=sync_state.json
# SYNC_LOG_LEVEL=INFO

# MongoDB Collections
MONGODB_CONVERSATIONS_COLLECTION=conversations
MONGODB_TRANSLATIONS_COLLECTION=translations
MONGODB_ANALYTICS_REPORTS_COLLECTION=analytics_reports
MONGODB_USER_ANALYTICS_COLLECTION=user_analytics

# S3 Storage Configuration
S3_ENABLED=true
S3_BUCKET=your-analytics-bucket
S3_PREFIX=conversation-analytics
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_REGION=us-east-1

# Parquet Optimization
PARQUET_STORAGE_ENABLED=true
PARQUET_BASE_DIR=./data/parquet
PARQUET_PARTITION_BY=year,month,day
PARQUET_COMPRESSION=snappy
PARQUET_ROW_GROUP_SIZE=100000
PARQUET_PAGE_SIZE=8192
PARQUET_TARGET_FILE_SIZE_MB=128
PARQUET_MAX_RECORDS_PER_FILE=50000

# Multi-Threading Configuration
ENABLE_MULTITHREADING=true
MAX_WORKERS=8
CHUNK_SIZE=1000
IO_THREADS=4
PROCESSING_THREADS=4

# GPU Configuration
ENABLE_GPU=false
GPU_MEMORY_LIMIT=4096  # MB
CUDA_VISIBLE_DEVICES=0  # Comma-separated list of GPU indices to use

# Batch Processing Configuration
BATCH_SIZE=100
MAX_RETRIES=3
RETRY_DELAY=2  # seconds

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=analytics_processing.log

DIFY_API_KEY=app-yogLaX0ezDv3DwaBLjXOhOZU
DIFY_BASE_URL=http://localhost/v1
